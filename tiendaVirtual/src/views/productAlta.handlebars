<form action="/api/products/register/register" method="post">

  <label for="title">Título:</label>
  <input type="text" required name="title" id="title" placeholder="Título" /><br />

  <label for="description">Descripción:</label>
  <input type="text" required name="description" id="description" placeholder="Descripción" /><br />

  <label for="code">Código:</label>
  <input type="text" required name="code" id="code" placeholder="Código" /><br />

  <label for="price">Precio:</label>
  <input type="number" required name="price" id="price" placeholder="Precio" /><br />

  <label for="status">Estado (true/false):</label>
  <input type="text" required name="status" id="status" placeholder="Estado (true/false)" /><br />

  <label for="stock">Stock:</label>
  <input type="number" required name="stock" id="stock" placeholder="Stock" /><br />

  <label for="category">Categoría:</label>
  <input type="text" required name="category" id="category" placeholder="Categoría" /><br />

  <button type="submit">Registrar producto</button><br />
</form>